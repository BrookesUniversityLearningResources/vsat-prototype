{
  "name": "@vsp/backend",
  "version": "1.0.0",
  "description": "Backend for the Virtual Storytelling Project",
  "main": "main.js",
  "repository": "https://github.com/richdouglasevans/vsp.git",
  "private": true,
  "license": "CC0-1.0",
  "scripts": {
    "db:up": "node run-db-migrate.js -- up",
    "db:up:test": "node run-db-migrate.js -- up:test",
    "db:down": "node run-db-migrate.js -- down",
    "db:down:test": "node run-db-migrate.js -- down:test",
    "db:create": "node run-db-migrate.js -- create",
    "prestart": "yarn db:up",
    "start": "node --inspect ./main.js",
    "pretest": "NODE_ENV=test yarn db:down && yarn db:down:test && yarn db:up && yarn db:up:test",
    "test": "yarn test:unit",
    "test:unit": "NODE_ENV=test jest --group=unit",
    "test:grammar": "NODE_ENV=test jest --group=grammar",
    "test:view": "NODE_ENV=test jest --group=view",
    "test:integration": "NODE_ENV=test jest --group=integration",
    "test:cloudinary": "NODE_ENV=test jest --group=cloudinary",
    "test:all": "yarn test:unit && yarn test:integration"
  },
  "dependencies": {
    "@magic-sdk/admin": "^1.8.0",
    "cloudinary": "^1.32.0",
    "config": "^3.3.8",
    "connect-pg-simple": "^8.0.0",
    "continuation-local-storage": "^3.2.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "execa": "^4.1.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "minimist": "^1.2.7",
    "multer": "^1.4.5-lts.1",
    "nunjucks": "^3.2.3",
    "ow": "0.19.0",
    "passport": "^0.6.0",
    "passport-magic": "^1.0.0",
    "pg": "^8.8.0",
    "pino": "^8.7.0",
    "ramda": "^0.28.0",
    "sanctuary": "^3.1.0"
  },
  "devDependencies": {
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.14",
    "@types/pg": "^7.14.11",
    "@types/sanctuary": "^3.1.0",
    "@types/supertest": "^2.0.12",
    "deep-freeze": "^0.0.1",
    "jest": "^29.3.1",
    "jest-docblock": "^29.2.0",
    "jest-runner": "^29.3.1",
    "jest-runner-groups": "^2.2.0",
    "supertest": "^6.3.2",
    "typescript": "^4.9.3"
  }
}
